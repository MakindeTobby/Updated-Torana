<IfModule mod_rewrite.c>
  RewriteEngine On # Redirect www to non-www RewriteCond %{HTTP_HOST}
  ^www\.toranacareaustralia\.com\.au [NC] RewriteRule ^(.*)$
  https://toranacareaustralia.com.au/$1 [R=301,L] # Existing rules RewriteBase /
  RewriteRule ^index\.html$ - [L] RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d RewriteRule . /index.html [L]
</IfModule>
